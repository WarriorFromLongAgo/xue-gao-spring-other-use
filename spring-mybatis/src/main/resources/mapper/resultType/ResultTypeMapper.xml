<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xuegao.springmybatis.business.result.mapper.ResultMapper">
    <sql id="BaseTable">
        mybatis_result
    </sql>

    <select id="list" resultType="com.xuegao.springmybatis.model.demo.doo.MybatisResult">
        select
        id,
        result_type
        from
        <include refid="BaseTable"/>
        limit 200
    </select>

    <select id="listMap" resultType="map">
        select
        id,
        result_type
        from
        <include refid="BaseTable"/>
        limit 200
    </select>
</mapper>